      "0\n",
      "1\n",
      "10\n",
      "Error: the counter has surpassed the maximum of 10\n",
      "Counter reset to 0\n",
      "0\n",
      "1\n",
      "10\n",
      "Error: the counter has surpassed the maximum of 10\n"
     ]
    }
   ],
   "source": [
    "class counter:\n",
    "    def __init__(self, start, max):\n",
    "        self.start = start\n",
    "        self.max = max\n",
    "        self.current = start\n",
    "\n",
    "    def inc(self, inc_amount):\n",
    "        if self.current == self.start:\n",
    "            print(self.current)\n",
    "        if self.current + inc_amount <= self.max:\n",
    "            self.current += inc_amount\n",
    "            print(self.current)\n",
    "        else:\n",
    "            print(f'Error: the counter has surpassed the maximum of {self.max}')\n",
    "\n",
    "    def reset(self):\n",
    "        self.current = self.start\n",
    "        print(f'Counter reset to {self.start}')\n",
    "\n",
    "count = counter(0, 10)\n",
    "count.inc(1)\n",
    "count.inc(9)\n",
    "count.inc(7)\n",
    "count.reset()\n",
    "count.inc(1)\n",
    "count.inc(9)\n",
    "count.inc(7)"
   ]
