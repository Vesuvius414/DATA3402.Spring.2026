    "rectangle.__repr__ = lambda self: (\n",
    "    f\"rectangle({self.len()},{self.wid()},\"\n",
    "    f\"{self._rectangle__top_l},{self._rectangle__top_r},\"\n",
    "    f\"{self._rectangle__low_l},{self._rectangle__low_r})\"\n",
    ")\n",
    "\n",
    "circle.__repr__ = lambda self: (\n",
    "    f\"circle({self.rad()},{self.cent()})\"\n",
    ")\n",
    "\n",
    "triangle.__repr__ = lambda self: (\n",
    "    f\"triangle({self.base()},{self.top()},\"\n",
    "    f\"{self._triangle__low_l},{self._triangle__low_r})\"\n",
    ")\n",
    "\n",
    "CompoundShape.__repr__ = lambda self: (\n",
    "    f\"CompoundShape([{', '.join([repr(s) for s in self.shapes])}])\"\n",
    ")\n",
    "\n",
    "class RasterDrawing:\n",
    "    def __init__(self, shapes=None):\n",
    "        if shapes is None:\n",
    "            self.shapes = []\n",
    "        else:\n",
    "            self.shapes = shapes\n",
    "\n",
    "    def add_shape(self, shape_obj):\n",
    "        self.shapes.append(shape_obj)\n",
    "\n",
    "    def remove_shape(self, shape_obj):\n",
    "        if shape_obj in self.shapes:\n",
    "            self.shapes.remove(shape_obj)\n",
    "\n",
    "    def paint(self, canvas, char='*'):\n",
    "        for s in self.shapes:\n",
    "            s.paint(canvas, char)\n",
    "\n",
    "    def __repr__(self):\n",
    "        return f\"RasterDrawing([{', '.join([repr(s) for s in self.shapes])}])\"\n",
    "\n",
    "    def save(self, filename):\n",
    "        with open(filename, \"w\") as f:\n",
