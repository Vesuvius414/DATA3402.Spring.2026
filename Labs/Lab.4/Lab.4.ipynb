    "def f_y_coords(x, rad, cent):\n",
    "    y_coord = (rad ** 2 - (x - cent[0]) ** 2) ** (1/2)\n",
    "    return (y_coord  + cent[1], -y_coord + cent[1])\n",
    "\n",
    "def f_tri_l(y, top, low_l):\n",
    "    m = (top[1] - low_l[1])/(top[0] - low_l[0])\n",
    "    b = low_l[1] - m * low_l[0]\n",
    "    return (y - b) / m\n",
    "\n",
    "def f_tri_r(y, top, low_r):\n",
    "    m = (low_r[1] - top[1])/(low_r[0] - top[0])\n",
    "    b = low_r[1] - m * low_r[0]\n",
    "    return (y - b) / m\n",
    "\n",
